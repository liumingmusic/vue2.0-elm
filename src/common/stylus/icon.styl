@font-face 
  font-family: 'sell'
  src: url('../fonts/sell.eot?cfsjom');
  src: url('../fonts/sell.eot?cfsjom#iefix') format('embedded-opentype'), 
        url('../fonts/sell.ttf?cfsjom') format('truetype'), 
        url('../fonts/sell.woff?cfsjom') format('woff'), 
        url('../fonts/sell.svg?cfsjom#sell') format('svg')
  font-weight: normal
  font-style: normal

[class^="sell-"], [class*=" sell-"]
  font-family: 'sell' !important
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

